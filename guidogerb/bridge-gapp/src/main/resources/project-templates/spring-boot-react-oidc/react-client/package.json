{
  "name": "Bridgegapp Client",
  "version": "1.0.0",
  "description": "#{project.desciption}",
  "license": "ISC",
  "author": "Gary Gerber",
  "repository": {
    "type": "git",
    "url": "git@github.com/guidogerb/bridge-gapp.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --debug --display-error-details --progress --config webpack.dev.js",
    "cleaninstall_mac_linux": "./cleaninstall_mac.sh",
    "cleaninstall_win": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./cleaninstall_win.ps1",
    "copyDocker": "node ./copyDockerFiles.js",
    "createVersionFile": "node ./getGitVersion.js",
    "lint": "esw webpack.config.* src/main/webapp/js/react tools || exit 0",
    "lint:watch": "npm run lint -- --watch",
    "npm reinstall": "npm install",
    "prod_build": "./node_modules/.bin/webpack --display-error-details --progress --config webpack.prod.js",
    "serve": "nodemon -e html --ignore dist/ --ignore src/ server.js",
    "setup_artifactory": "npm config set registry https://bluepantsmedia.com/artifactory/api/npm/npm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "./node_modules/.bin/webpack --watch --debug --display-error-details --progress --config webpack.dev.js"
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11"
  ],
